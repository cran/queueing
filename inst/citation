year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{queueing}: Analysis of Queueing Networks and Models",
         author = c(person("Pedro", "Canadilla")),
         year = year,
         note = note,
         url = "http://CRAN.R-project.org/package=queueing")
